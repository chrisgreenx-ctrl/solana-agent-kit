{
  "name": "solana-agent-kit-gui",
  "version": "1.0.0",
  "description": "Solana Agent Kit v2 GUI - Complete web interface for blockchain operations",
  "main": "server/index.ts",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:mobile": "concurrently \"npm run server\" \"npm run expo\"",
    "server": "tsx server/index.ts",
    "client": "cd client && npm run dev",
    "expo": "cd mobile && npx expo start --web --port 5000",
    "build": "cd client && npm run build",
    "start": "NODE_ENV=production tsx server/index.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/openai": "^1.3.13",
    "@coral-xyz/anchor": "^0.29.0",
    "@langchain/core": "^0.3.44",
    "@langchain/langgraph": "^0.2.64",
    "@langchain/openai": "^0.5.5",
    "@react-navigation/native-stack": "^7.8.6",
    "@solana-agent-kit/plugin-blinks": "^2.0.5",
    "@solana-agent-kit/plugin-defi": "^2.0.8",
    "@solana-agent-kit/plugin-misc": "^2.0.6",
    "@solana-agent-kit/plugin-nft": "2.0.0-beta.11",
    "@solana-agent-kit/plugin-token": "2.0.0-beta.11",
    "@solana/web3.js": "^1.98.0",
    "@types/bn.js": "^5.2.0",
    "ai": "^4.3.8",
    "bn.js": "^5.2.2",
    "bs58": "^6.0.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.0",
    "node-fetch": "^3.3.1",
    "solana-agent-kit": "2.0.0-beta.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.1"
  }
}
